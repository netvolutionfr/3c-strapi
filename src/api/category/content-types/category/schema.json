{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "displayName": "Category",
    "singularName": "category",
    "pluralName": "categories",
    "description": "Sous-rubrique à l’intérieur d’une section (ex : Chimiothérapie, Fatigue, Droits, etc.)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "order": {
      "type": "integer"
    },
    "section": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::section.section",
      "required": true
    }
  }
}